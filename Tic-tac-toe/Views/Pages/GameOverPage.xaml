<UserControl x:Class="Tic_tac_toe.Views.Pages.GameOverPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Tic_tac_toe.Views.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="#fff">
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock x:Name="Result_TextBlock"></TextBlock>
            <Button x:Name="PlayAgain_Button" Click="PlayAgain_Button_Click">Play again</Button>
            <Button x:Name="Return_Button" Click="Return_Button_Click">Return to menu</Button>
        </StackPanel>
    </Grid>
</UserControl>
